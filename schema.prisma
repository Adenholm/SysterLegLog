datasource db {
    provider = "sqlite"
    url      = "file:./sqlite.db"
}

generator client {
    provider = "prisma-client-js"
}

model Category {
    id    Int    @id @default(autoincrement())
    name  String @unique
    pic   String
    cards Card[]
}

model Card {
    id         Int      @id @default(autoincrement())
    name       String   @unique
    pic        String
    sound      String
    category   Category @relation(fields: [categoryId], references: [id])
    categoryId Int
}
